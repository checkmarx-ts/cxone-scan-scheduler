{
	"version": 1,
	"handlers": {
		"null": {
			"class": "logging.NullHandler"
		},
		"console": {
			"class": "logging.StreamHandler",
			"formatter": "default",
			"stream": "ext://sys.stdout"
		}

	},
	"formatters": {
		"default": {
			"format": "[%(asctime)s][%(process)d][%(name)s][%(levelname)s] %(message)s",
			"datefmt": "%Y-%m-%dT%H:%M:%S%z"
		}
	},
	"loggers": {
		"root": {
			"handlers": ["null"],
			"level": "INFO"
		},
		"audit": {
			"handlers": ["console"],
			"level": "ERROR"
		}
	}
}
